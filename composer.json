{
  "name": "akkurateio/laravel-postman",
  "description": "Generate a Postman collection automatically from your Laravel API",
  "keywords": [
    "laravel",
    "postman",
    "api",
    "collection",
    "generate"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Subvitamine",
      "email": "cto@subvitamine.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Akkurateio\\LaravelPostman\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Akkurateio\\LaravelPostman\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^7.4|^8.0",
    "ext-json": "*",
    "illuminate/config": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/console": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/support": "^6.0|^7.0|^8.0|^9.0",
    "illuminate/routing": "^6.0|^7.0|^8.0|^9.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Akkurateio\\LaravelPostman\\LaravelPostmanServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "orchestra/testbench": "^6.12"
  }
}